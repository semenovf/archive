<?xml version="1.0" encoding="UTF-8"?>
<metapage>
    <use webject="JQ::Webject::LineEdit" as="LineEdit" />
    <use webject="JQ::Webject::Label" as="Label" />
    <use webject="JQ::Webject::Table" as="Table"/>

    <auth role="admin">
        <!-- some code for registered user with 'admin' role -->
        <Label where="before" value="Some label:">
            <LineEdit name="" size="" maxLength="" value="" />
        </Label>
    </auth>

    <auth role="registered">
        <!-- some code for registered users -->
    </auth>

    <auth perm="CanViewCodeBelowPerm">
        <!-- some code for user with permission 'CanViewCodeBelowPerm' -->
    </auth>

    <auth role="superuser" perm="CanViewCodeBelowPerm">
        <!-- some code for user with permission 'CanViewCodeBelowPerm' -->
        <!-- permission is prefered than role -->
        <!-- is equivalent to &lt;auth perm="CanViewCodeBelowPerm"&rt; -->
    </auth>

</metapage>
